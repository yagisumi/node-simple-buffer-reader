{
  "name": "@yagisumi/simple-buffer-reader",
  "version": "1.0.0",
  "description": "simple buffer reader",
  "keywords": [],
  "main": "lib/simple-buffer-reader.js",
  "types": "lib/simple-buffer-reader.d.ts",
  "unpkg": "umd/simple-buffer-reader.js",
  "scripts": {
    "test": "jest",
    "doc": "typedoc  --tsconfig ./lib/tsconfig.json --json ./docs/typedoc.json ./lib",
    "build": "run-s build:*",
    "clean": "rimraf coverage umd {lib,test}/**/*.{js,map,d.ts} *.tgz",
    "build:lib": "tsc -p lib",
    "build:test": "tsc -p test",
    "build:umd": "webpack",
    "tslint": "tslint --exclude **/*.d.ts lib/**/*.{ts,tsx} test/**/*.ts",
    "tslint:json": "tslint --format json --exclude **/*.d.ts lib/**/*.{ts,tsx} test/**/*.ts",
    "tslint:fix": "tslint --fix --exclude **/*.d.ts lib/**/*.{ts,tsx} test/**/*.ts",
    "prepack": "run-s build test"
  },
  "author": "Yagi Sumiya",
  "repository": "github:yagisumi/node-simple-buffer-reader",
  "homepage": "http://github.com/yagisumi/node-simple-buffer-reader#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2"
  }
}
